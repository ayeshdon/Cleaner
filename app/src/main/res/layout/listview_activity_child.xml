<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/childDataTextView"
        android:paddingTop="5dp"
        android:paddingLeft="10dp"
        android:textColor="@color/text_black"
        android:fontFamily="sans-serif-light"
        android:paddingBottom="5dp"/>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="20dp"
            android:gravity="center"
            android:id="@+id/subActivityImageView"
            android:src="@drawable/sub_activities" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/feedBackImageViedw"
            android:padding="20dp"
            android:gravity="center"
            android:src="@drawable/comment_icon" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="20dp"
            android:gravity="center"
            android:id="@+id/postponeImageView"
            android:src="@drawable/postpone" />

        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="@string/more_info"
            android:paddingRight="15dp"
            android:gravity="center|right"
            android:textColor="@color/btn_singup_bg"
            android:id="@+id/moreDetailsTextView" />

    </LinearLayout>


</LinearLayout>
